{
    "name": "cncal/alipay",
    "description": "alipay(web) for laravel.",
    "keywords": ["laravel", "alipay"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Liu Calvin",
            "email": "flycalvin@qq.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "illuminate/support": ">= 5.1"
